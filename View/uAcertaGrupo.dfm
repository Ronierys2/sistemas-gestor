  object qryGrupo: TFDQuery
    Connection = Dados.Conexao
    SQL.Strings = (
      'select CODIGO, DESCRICAO from('
      'select g.codigo, g.descricao  from grupo g'
      'union'
      
        'select (0) as CODIGO, ('#39'<todos os grupos>'#39') as DESCRICAO from RD' +
        'B$DataBase'
      ')'
      'where'
      'CODIGO>=0'
      'order by CODIGO')
    Left = 416
    Top = 64
